pipeline{
    agent any
 environment {
         VERCEL_TOKEN=credentials('vercel_token')   
 }   
 stages{
    stage('install_dependencies'){
        steps{
            bat 'npm install'
        }
    }
    stage('tese'){
        steps{
            echo "skipping test stagesssss some change "
        }
    }
    stage('build'){
        steps{
            bat 'npm run build'
        }
    }
    stage('deploy'){
        steps{
            bat 'npm vercel --prod --yes --token=%VERCEL_TOKEN%'
        }
    }
 }
}